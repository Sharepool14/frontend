{
	"name": "svelte-node-template",
	"version": "0.0.1",
	"quokka": {
		"babel": true
	},
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"format:watch": "onchange \"**/*.svelte\" \"**/*.ts\" -- prettier --write --ignore-unknown {{changed}}",
		"serve": "node server/index.js",
		"b:serve": "svelte-kit build && node server/index.js"
	},
	"devDependencies": {
		"@fullhuman/postcss-purgecss": "^4.1.3",
		"@playwright/test": "^1.20.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-node": "^1.0.0-next.73",
		"@sveltejs/kit": "1.0.0-next.328",
		"@types/cookie": "^0.5.1",
		"@types/js-cookie": "^3.0.2",
		"@types/node": "^17.0.23",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.4",
		"cssnano": "^5.1.5",
		"cssnano-preset-advanced": "^5.3.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"postcss-load-config": "^3.1.4",
		"postcss-nested": "^5.0.1",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"purgecss-from-svelte": "^2.0.2",
		"sass": "^1.51.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"type": "module",
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.1.1",
		"@fortawesome/free-solid-svg-icons": "^6.1.1",
		"cookie": "^0.5.0",
		"js-cookie": "^3.0.1",
		"onchange": "^7.1.0",
		"svelte-fa": "^2.4.0"
	}
}
